#include <iostream>
using namespace std;
int main() {
    int num;
    cin>>num;
    switch (num < 0){
        case 1:
            cout<<"Negative"<<endl;
            break;

            switch (num > 0) {
                    
                case 0:
                    cout<<"Zero"<<endl;
                    break;
                    
                
            }
    }
    return 0;
}
